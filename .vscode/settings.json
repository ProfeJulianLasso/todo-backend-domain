{
  "jest.autoRun": "off",
  "iridium.npm.packageManager": "pnpm",
  "files.exclude": {
    "**/node_modules": true
  },
  "explorer.fileNesting.patterns": {
    "package.json": ".*,*.json,*.yaml,*yml,*.js,+.json,*.md,Gemfile,*.properties,*.lock,LICENSE,bun.lockb",
    "*.d.ts": "$(capture).js,$(capture).js.map",
  },
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders",
    "pnpm-lock.yaml",
    ".eslintrc.json"
  ],
  "cSpell.words": [
    "commitlint",
  ],
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.preferences.importModuleSpecifierEnding": "minimal",
  "typescript.preferences.quoteStyle": "single",
  "duplicatedCode.options": {
    "minTokens": 90,
    "minLines": 5,
    "maxLines": 1000,
    "absolute": true,
    "silent": true,
    "gitignore": true,
    "noSymlinks": false
  }
}